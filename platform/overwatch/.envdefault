# Overwatch settings
# Default 80
LINTO_STACK_OVERWATCH_HTTP_PORT=
LINTO_STACK_OVERWATCH_LOGS_MONGODB=false

# Overwatch auth settings
LINTO_STACK_OVERWATCH_JWT_SECRET=secret
LINTO_STACK_OVERWATCH_REFRESH_SECRET=refresh
LINTO_STACK_OVERWATCH_AUTH_TYPE=local

LINTO_STACK_OVERWATCH_AUTH_LDAP_SERVER_URL=
LINTO_STACK_OVERWATCH_AUTH_LDAP_SERVER_SEARCH_BASE=
LINTO_STACK_OVERWATCH_AUTH_LDAP_SERVER_SEARCH_FILTER=

LINTO_STACK_OVERWATCH_BASE_PATH=/overwatch

# MQTT settings
LINTO_STACK_MQTT_HOST=localhost
LINTO_STACK_MQTT_PORT=1883
LINTO_STACK_MQTT_KEEP_ALIVE=2
LINTO_STACK_MQTT_USE_LOGIN=false
LINTO_STACK_MQTT_USER=
LINTO_STACK_MQTT_PASSWORD=

LINTO_STACK_OVERWATCH_DEVICE_TOPIC_KEY=DEV_
LINTO_STACK_OVERWATCH_WEB_TOPIC_KEY=WEB_

# MQTT WS settings
LINTO_STACK_WSS=false
LINTO_STACK_MQTT_OVER_WS=false
LINTO_STACK_MQTT_OVER_WS_ENDPOINT=/mqtt

# Mongo settings
LINTO_STACK_MONGODB_SERVICE=127.0.0.1
LINTO_STACK_MONGODB_PORT=27017
LINTO_STACK_MONGODB_USE_LOGIN=false
LINTO_STACK_MONGODB_USER=
LINTO_STACK_MONGODB_PASSWORD=

LINTO_STACK_MONGODB_DBNAME=linto
LINTO_STACK_MONGODB_COLLECTION_LINTOS=lintos
LINTO_STACK_MONGODB_COLLECTION_LOG=statusLog
